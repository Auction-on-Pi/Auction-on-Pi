generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"  // Add this line
}
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Auction {
  id        Int      @id @default(autoincrement())
  title     String
  bids      Bid[]
}
model Bid {
  id        Int      @id @default(autoincrement())
  amount    Float
  auction   Auction  @relation(fields: [auctionId], references: [id])
  auctionId Int
}
